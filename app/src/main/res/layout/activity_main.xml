<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">

    <ListView
        android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-38dp"
        tools:ignore="MissingConstraints" />

    <ListView
        android:id="@+id/action_search"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ImageLogo"
        android:layout_width="73dp"
        android:layout_height="83dp"
        android:contentDescription="@string/app_logo"
        android:src="@drawable/logo"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.SearchView
        android:id="@+id/search"
        android:layout_width="256dp"
        android:layout_height="57dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toEndOf="@id/ImageLogo"
        app:layout_constraintTop_toTopOf="parent"
        app:menu="@menu/menu" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/accomodation"
        android:layout_width="180dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/button_bg"
        android:text="Accomodation"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ImageLogo" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buy_and_sale"
        android:layout_width="180dp"
        android:layout_height="61dp"
        android:background="@drawable/button_bg"
        android:text="Buy/sale"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@id/accomodation"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toBottomOf="@id/ImageLogo"
        android:layout_marginTop="32dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/lost_and_found"
        android:layout_width="180dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_bg"
        android:text="Lost-And-Found"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/accomodation" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/project"
        android:layout_width="180dp"
        android:layout_height="61dp"
        android:background="@drawable/button_bg"
        android:text="Project"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/buy_and_sale"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toEndOf="@id/lost_and_found"
        android:layout_marginStart="20dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:layout_width="180dp"
        android:layout_height="61dp"
        android:background="@drawable/button_bg"
        android:text="Other"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/project"
        app:layout_constraintStart_toEndOf="@id/services"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/services"
        android:layout_width="180dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/button_bg"
        android:text="Services"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lost_and_found" />

    <androidx.fragment.app.FragmentContainerView
        android:layout_width="198dp"
        android:layout_height="409dp"
        android:background="@color/whhhw"
        android:radius="20dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="365dp"
        tools:ignore="MissingConstraints" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerView"
        android:layout_width="205dp"
        android:layout_height="413dp"
        android:background="@color/whhhw"
        android:radius="20dp"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="422dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="183dp"
        android:layout_height="244dp"
        android:src="@drawable/img"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="374dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:layout_width="183dp"
        android:layout_height="136dp"
        android:text="Продам 3-х комнатную квартиру в центре города \n 19 500 000 тенге"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginStart="16dp"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/services" />


</androidx.constraintlayout.widget.ConstraintLayout>
